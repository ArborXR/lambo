{
    "name": "tightenco/lambo",
    "description": "Super-powered 'laravel new' with Laravel and Valet.",
    "keywords": ["laravel", "zonda", "wwdhhd", "lambo"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tightenco/lambo/issues",
        "source": "https://github.com/tightenco/lambo"
    },
    "authors": [
        {
            "name": "Matt Stauffer",
            "email": "matt@mattstauffer.co"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/database": "5.5.*",
        "laravel-zero/framework": "4.0.*",
        "tivie/php-os-detector": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["lambo"]
}
